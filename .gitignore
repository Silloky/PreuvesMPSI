# Ignore everything inside .obsidian
.obsidian/*

# Allow traversal into needed subdirectories
!.obsidian/
!.obsidian/plugins/
!.obsidian/plugins/obsidian-git/
.obsidian/plugins/obsidian-git/*
!.obsidian/plugins/obsidian-latex-suite/
.obsidian/plugins/obsidian-latex-suite/*
!.obsidian/plugins/math-in-callout
.obsidian/plugins/math-in-callout/*
!.obsidian/snippets/

# Whitelist only these files
!.obsidian/core-plugins.json
!.obsidian/community-plugins.json
!.obsidian/hotkeys.json
!.obsidian/types.json
!.obsidian/app.json
!.obsidian/appearance.json
!.obsidian/templates.json
!.obsidian/plugins/obsidian-git/data.json
!.obsidian/plugins/obsidian-latex-suite/data.json
!.obsidian/plugins/math-in-callout/data.json